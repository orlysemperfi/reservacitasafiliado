<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
  <class name="Afiliado" table="Afiliado">
    <id name="IdAfiliado" column="IdAfiliado">
      <generator class="increment" />
    </id>
    <property name="DNI" column="DNI" />
    <property name="Nombre" column="Nombre" />
    <property name="ApePaterno" column="APaterno" />
    <property name="ApeMaterno" column="AMaterno" />
    <property name="Direccion" column="Direccion" />
    <property name="FechaNacimiento" column="FechaNacimiento" />
    <property name="Estado" column="Estado" />
    
    <!--<many-to-one name="Sede" column="sede" />-->
  </class>
</hibernate-mapping>
