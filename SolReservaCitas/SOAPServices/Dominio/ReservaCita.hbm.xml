<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
	<class name="ReservaCita" table="ReservaCita">
		<id name="IdReservaCita" column="IdReservaCita">
			<generator class="identity" />
		</id>
		<property name="IdAfiliado" column="IdAfiliado" />
		<property name="IdCentroAtencion" column="IdCentroAtencion" />
		<property name="IdMedico" column="IdMedico" />
		<property name="IdConsultorio" column="IdConsultorio" />
		<property name="FechaAsignada" column="FechaAsignada" />
		<property name="Observacion" column="Observacion" />
		<property name="Estado" column="Estado" />

		<!--<many-to-one name="Sede" column="sede" />-->
	</class>
</hibernate-mapping>
