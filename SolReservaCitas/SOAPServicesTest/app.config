<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAfiliado" />
                <binding name="BasicHttpBinding_IReservaCitaService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:5000/Implementacion/AfiliadoService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAfiliado"
                contract="AfiliadoProxy.IAfiliado" name="BasicHttpBinding_IAfiliado" />
            <endpoint address="http://localhost:5000/Implementacion/ReservaCitaService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IReservaCitaService"
                contract="ReservaProxy.IReservaCitaService" name="BasicHttpBinding_IReservaCitaService" />
        </client>
    </system.serviceModel>
</configuration>